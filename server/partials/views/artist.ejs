<img ng-src="{{artistinfo.images[2].url}}" class="pull-left img-circle" width="200" height="200" />
<h2>{{artistinfo.name}}</h2>
<table class="table table-striped table-hover">
	<tr>
		<td>
		#
		</td>
		<td>
		Nummer
		</td>
	</tr>
	<tr ng-repeat="track in artisttoptracks.tracks track by $index" ng-click="play($index, artistinfo.name + ' - ' + track.name)" ng-if="$index < 5">
		<td>
		{{$index+1}}
		</td>
		<td>
		{{track.name}}<ins class="pull-right " ng-show="track.explicit">explicit</ins>
		</td>
	</tr>
</table>